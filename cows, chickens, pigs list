function printedBase(number, label){
    let numberString= String(number);
    while(numberString.length<3){
        numberString="0"+numberString;
    }
    console.log(`${numberString} ${label}`);
    
}

function printedActual(cows, chickens, pigs){
    printedBase(cows, "cows");
    printedBase(chickens, "chickens");
    printedBase(pigs,"pigs");
    
}
printedActual(7,11,13)
