//Written in Python
def findThePhoneNumber():
    message=input("what's the statement: ")
    print("\n")
    for i in range(len(message)):
        chunk=message[i:i+12]
        if isAPhoneNumber(chunk):
            print('phone number found '+chunk)
def isAPhoneNumber(text):
    if len(text)!=12:
        return False
    for i in range(0,3):
        if not text[i].isdecimal():
            return False
    if text[3]!="-":
        return False
    for i in range(4,7):
        if not text[i].isdecimal():
            return False
    if text[7]!="-":
        return False
    for i in range(8,12):
        if not text[i].isdecimal():
            return False
    return True


findThePhoneNumber()
