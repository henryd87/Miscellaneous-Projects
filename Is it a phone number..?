//Written in Python
def isAPhoneNumber(text):
    if len(text)!=12:
        return False
    for i in range(0,3):
        if not text[i].isdecimal():
            return False
    if text[3]!="-":
        return False
    for i in range(4,7):
        if not text[i].isdecimal():
            return False
    if text[7]!="-":
        return False
    for i in range(8,12):
        if not text[i].isdecimal():
            return False
    return True
print('is 214-789-0987 a phone number?')
print(isAPhoneNumber('214-789-0987'))
